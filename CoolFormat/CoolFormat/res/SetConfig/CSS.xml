<SETCONFIG>
	<VERSION>1</VERSION>
	<PROPERTYS>
		<PROPERTY>
			<NAME>Code Layout</NAME>
			<GROUP>TRUE</GROUP>
		</PROPERTY>
		<PROPERTY>
			<NAME>Compression</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>Highest</VALUE>
			<ITEM>
				<VALUE>Highest</VALUE>
				<SHORT>c0</SHORT>
				<PREVIEW>h1{color:red;}h2{color:black;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>High</VALUE>
				<SHORT>c1</SHORT>
				<PREVIEW>h1{color:red;}\r\nh2{color:black;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>Standard</VALUE>
				<SHORT>c2</SHORT>
				<PREVIEW>h1 {\r\ncolor:red;\r\n}\r\n\r\nh2 {\r\ncolor:black;\r\n}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>Low</VALUE>
				<SHORT>c3</SHORT>
				<PREVIEW>h1\r\n{\r\n\tcolor:red;\r\n}\r\n\r\nh2\r\n{\r\n\tcolor:black;\r\n}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>Custom</VALUE>
				<SHORT>c4</SHORT>
				<PREVIEW>Use custom template</PREVIEW>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>Custom template</NAME>
			<TYPE>Text</TYPE>
			<SHORT>ct</SHORT>
			<PREVIEW>Example:\r\n\r\n&lt;span class=&quot;at&quot;&gt;|&lt;/span&gt; &lt;span class=&quot;format&quot;&gt;{&lt;/span&gt;\r\n|&lt;span class=&quot;selector&quot;&gt;|&lt;/span&gt; &lt;span class=&quot;format&quot;&gt;{&lt;/span&gt;\r\n|&lt;span class=&quot;property&quot;&gt;|&lt;/span&gt;&lt;span class=&quot;value&quot;&gt;|&lt;/span&gt;&lt;span class=&quot;format&quot;&gt;;&lt;/span&gt;\r\n|&lt;span class=&quot;format&quot;&gt;}&lt;/span&gt;|\r\n\r\n|\r\n&lt;span class=&quot;format&quot;&gt;}&lt;/span&gt;\r\n\r\n||&lt;span class=&quot;comment&quot;&gt;|&lt;/span&gt;\r\n|\r\n|</PREVIEW>
		</PROPERTY>
		<PROPERTY>
			<NAME>Options</NAME>
			<GROUP>TRUE</GROUP>
		</PROPERTY>
		<PROPERTY>
			<NAME>Preserve CSS</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>No</VALUE>
			<ITEM>
				<VALUE>Yes</VALUE>
				<SHORT>pc</SHORT>
				<PREVIEW>/* color */\r\nh1 {\r\ncolor:red;\r\n}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>No</VALUE>
				<PREVIEW>h1 {\r\ncolor:red;\r\n}</PREVIEW>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>Sort selectors(caution)</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>No</VALUE>
			<ITEM>
				<VALUE>Yes</VALUE>
				<SHORT>ss</SHORT>
				<PREVIEW>h2{color:black;}\r\nh1{color:red;}\r\n==&gt;\r\nh1{color:red;}\r\nh2{color:black;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>No</VALUE>
				<PREVIEW>h2{color:black;}\r\nh1{color:red;}\r\n==&gt;\r\nh2{color:black;}\r\nh1{color:red;}</PREVIEW>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>Sort properties</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>No</VALUE>
			<ITEM>
				<VALUE>Yes</VALUE>
				<SHORT>sp</SHORT>
				<PREVIEW>h1{font-size:14px;color:red;}\r\n==&gt;\r\nh1{color:red;font-size:14px;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>No</VALUE>
				<PREVIEW>h1{font-size:14px;color:red;}\r\n==&gt;\r\nh1{font-size:14px;color:red;}</PREVIEW>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>Remove last ;</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>No</VALUE>
			<ITEM>
				<VALUE>Yes</VALUE>
				<SHORT>rl</SHORT>
				<PREVIEW>h1{color:red}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>No</VALUE>
				<PREVIEW>h1{color:red;}</PREVIEW>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>Remove unnecessary backslashes</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>No</VALUE>
			<ITEM>
				<VALUE>Yes</VALUE>
				<SHORT>rub</SHORT>
				<PREVIEW>h1{color:blue;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>No</VALUE>
				<PREVIEW>h1{color:red;c\olor:blue;}</PREVIEW>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>Discard invalid properties</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>No</VALUE>
			<ITEM>
				<VALUE>Yes</VALUE>
				<SHORT>dip</SHORT>
				<PREVIEW>h1{color:red;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>No</VALUE>
				<PREVIEW>h1{color:red;font-size-adjust:.58;}</PREVIEW>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>CSS level</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>CSS2.1</VALUE>
			<ITEM>
				<VALUE>CSS2.1</VALUE>
				<SHORT>cl0</SHORT>
			</ITEM>
			<ITEM>
				<VALUE>CSS2.0</VALUE>
				<SHORT>cl1</SHORT>
			</ITEM>
			<ITEM>
				<VALUE>CSS1.0</VALUE>
				<SHORT>cl2</SHORT>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>Silent</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>No</VALUE>
			<ITEM>
				<VALUE>Yes</VALUE>
				<SHORT>s</SHORT>
				<PREVIEW>No errors are shown</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>No</VALUE>
				<PREVIEW>Errors are shown</PREVIEW>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>Optimise shorthands</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>Safe optimisations</VALUE>
			<ITEM>
				<VALUE>Don't optimise</VALUE>
				<SHORT>os0</SHORT>
				<PREVIEW>h1{margin-top:10px;margin-right:20px;margin-bottom:10px;margin-left:20px;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>Safe optimisations</VALUE>
				<SHORT>os1</SHORT>
				<PREVIEW>h1{margin:10px 20px;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>All optimisations</VALUE>
				<SHORT>os2</SHORT>
				<PREVIEW>h1{margin:10px 20px;}</PREVIEW>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>Compress colors</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>No</VALUE>
			<ITEM>
				<VALUE>Yes</VALUE>
				<SHORT>cc</SHORT>
				<PREVIEW>h1{color:red;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>No</VALUE>
				<PREVIEW>h1{color:#ff0000;}</PREVIEW>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>Compress font weight</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>No</VALUE>
			<ITEM>
				<VALUE>Yes</VALUE>
				<SHORT>cf</SHORT>
				<PREVIEW>h1{font-weight:700;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>No</VALUE>
				<PREVIEW>h1{font-weight:bold;}</PREVIEW>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>Lowercase selectors</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>No</VALUE>
			<ITEM>
				<VALUE>Yes</VALUE>
				<SHORT>ls</SHORT>
				<PREVIEW>h1{color:red;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>No</VALUE>
				<PREVIEW>H1{color:red;}</PREVIEW>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>Add timestamp</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>No</VALUE>
			<ITEM>
				<VALUE>Yes</VALUE>
				<SHORT>at</SHORT>
				<PREVIEW>/* CSSTidy 1.3: Sun Sep 02 17:16:30 2012 */\r\nh1{color:red;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>No</VALUE>
				<PREVIEW>h1{color:red;}</PREVIEW>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>Case for properties</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>None</VALUE>
			<ITEM>
				<VALUE>None</VALUE>
				<SHORT>cfp0</SHORT>
				<PREVIEW>h1{COLor:red;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>Lowercase</VALUE>
				<SHORT>cfp1</SHORT>
				<PREVIEW>h1{color:red;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>Uppercase</VALUE>
				<SHORT>cfp2</SHORT>
				<PREVIEW>h1{COLOR:red;}</PREVIEW>
			</ITEM>
		</PROPERTY>
		<PROPERTY>
			<NAME>Regroup selectors</NAME>
			<TYPE>Combo</TYPE>
			<VALUE>Merge same</VALUE>
			<ITEM>
				<VALUE>Do not change</VALUE>
				<SHORT>rs0</SHORT>
				<PREVIEW>h1{color:red;}\r\nh2{color:red;}\r\n==&gt;\r\nh1{color:red;}\r\nh2{color:red;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>Split at ,</VALUE>
				<SHORT>rs1</SHORT>
				<PREVIEW>h1,h2{color:red;}\r\n==&gt;\r\nh1{color:red;}\r\nh2{color:red;}</PREVIEW>
			</ITEM>
			<ITEM>
				<VALUE>Merge same</VALUE>
				<SHORT>rs2</SHORT>
				<PREVIEW>h1{color:red;}\r\nh2{color:red;}\r\n==&gt;\r\nh1,h2{color:red;}</PREVIEW>
			</ITEM>
		</PROPERTY>
	</PROPERTYS>
</SETCONFIG>